package lab1;

public class lab1{
	
	public static void main (String[]args){
		byte a = 3, b = 5;
		short m = 10, n = 15;
		double result = 0;	
				   
		final byte c = 3;
											
		for (short i = a; i < n; i++){	
			if (i - c == 0){ 
				System.out.print("Division by ");
				break;
				}  
			for (short j = b; j < m; j++){	 
				result += (double)(i % j) / (i - c);   
			}
		 }
		System.out.println(result);  
	} 
}
